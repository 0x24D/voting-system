{
    "root": true,
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "extends": [
        "plugin:vue/essential",
        "@vue/airbnb"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "no-console": "off",
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    }
}
